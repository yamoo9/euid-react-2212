{
  "private": true,
  "name": "webpack-react-typescript",
  "version": "1.0.0",
  "description": "Webpack 모듈 번들러를 사용해 React, TypeScript 등 프론트엔드 개발에 필요한 환경을 직접 구성합니다.",
  "engines": {
    "node": "^16.0.0",
    "vscode": "^1.22.0"
  },
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "webpack serve --config ./webpack/config.js --env env=dev",
    "build": "webpack build --config ./webpack/config.js --env env=prod"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-router-dom": "5.3.3",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.0",
    "css-loader": "6.7.2",
    "postcss": "8.4.19",
    "postcss-loader": "7.0.2",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "tailwindcss": "3.2.4",
    "ts-loader": "9.4.2",
    "typescript": "4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0",
    "webpack-dev-server": "4.11.1"
  },
  "keywords": [
    "webpack",
    "react",
    "typescript"
  ],
  "license": "MIT"
}
